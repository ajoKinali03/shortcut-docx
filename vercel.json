{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    { "src": "public/**", "use": "@vercel/static" },
    { "src": "views/**", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    },
    {
      "src": "/(.+)",
      "dest": "public/$1"
    },
    {
      "src": "/(.+)",
      "dest": "views/$1"
    }
  ]
}
